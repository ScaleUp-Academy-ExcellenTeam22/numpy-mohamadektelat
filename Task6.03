import numpy as np


def matrix_size(matrix) -> tuple[int, int]:
    """
    This program finds the number of rows and columns of a given matrix.
    :param matrix: tuple[int, int].
    :return: number of rows and columns.
    """
    return matrix.shape


if __name__ == '__main__':
    matrix = np.arange(20).reshape(5, 4)
    print(matrix_size(matrix))
